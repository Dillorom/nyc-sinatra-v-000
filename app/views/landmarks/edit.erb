<h1>Edit a Landmark</h1>
<form method="post" action="/landmarks/<%= @landmark.id %>/edit">
    <input type="hidden" name="_method" value="patch">
  <h1>Landmark Name:<%= @landmark.name %></h1>
  <input type="text" name="landmark[name]" id="landmark_name">
  <h1>Landmark Year Completed: <%= @landmark.year_completed %></h1>
    <input type="text" name="landmark[year_completed]" id="landmark_year_completed">

  <input type="submit" name="submit" value="Edit Landmark">
</form>
